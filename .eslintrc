{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier", "jest"],
  "globals": {
    "mixpanel": true
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": [0],
    "import/extensions": [0],
    "import/no-extraneous-dependencies": [0],
    "import/no-unresolved": [0],
    "prettier/prettier": [
      "error",
      { "singleQuote": true, "trailingComma": "es5" }
    ]
  }
}
